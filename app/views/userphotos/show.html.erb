<div class="page-header">
  <h3>Userphoto #<%= @userphoto.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Listing</dt>
      <dd>
        <% if @userphoto.listing.present? %>
          <a href="/listings/<%= @userphoto.listing_id %>">
            <%= @userphoto.listing.title %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/userphotos" class="btn btn-primary">
        Back
      </a>
      <a href="/userphotos/<%= @userphoto.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_userphoto/<%= @userphoto.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
